<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jeleren.dao.IImageLikeDao">

    <insert id="imageLike">
        insert into imagelike (image, user) values (#{image}, #{user})
    </insert>
    <delete id="cancelImageLike">
        delete from imagelike where image = #{image} and user = #{user}
    </delete>
    <select id="checkImageLiked" resultType="com.jeleren.bean.ImageLike">
        select * from imagelike where image = #{image} and user = #{user}
    </select>
</mapper>